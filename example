# Initial thoughs


SET TIME 1
SET MAX 10
SET INVERT OFF

CONSTANT sigma = 10
CONSTANT rho = 28
CONSTANT beta = 8 / 3

# dx = sigma * ( y - x ) 
#
E1 = SUM( Y , -X )
M1 = MULTIPLY( E1 , sigma )
X  = INTEGRATE (M1)

# dy = x * ( rho - z ) - y
#
E2 = SUM( rho , -Z )
M2 = MULTIPLY ( X , M1 )
E3 = SUM( E2 , -Y )
Y  = INTEGRATE( E3 )

# dz = x * y - beta * z
#
M3 = MULTIPLY( X , Y )
S4 = SCALE( Z , beta )
E4 = SUM( M3 , -M4 )
Z  = INTEGRATE( E4 )

PLOT(X,Y)

#------------------------------------------
#
FUNCTION SQRT( X )
M1 = MULTIPLY( S1 , S1 )
S1 = SUM( M1 , X )
RETURN( S1 )


FUNCTION SQR( X )
M1 = MULTIPLY( X , X )
RETURN( M1 )

FUNCTION DIVIDE( X , Y ) #divide x by y
# E1 = X / Y
E1 = SUM( X , M1 )
M1 = MULTIPLY( E1 , Y )
RETURN ( E1 )


FUNCTION CUBE( X )
# M2 = X * X * X
M1 = MULTIPLY( X , X )
M2 = MULTIPLY( X , M1 )
RETURN(M2)
       
FUNCTION SIN()
SET INVERT ON
X = INTEGRATE( J )
Y = INTEGRATE( X )
J = INVERT( Y )
RETURN( Y )
