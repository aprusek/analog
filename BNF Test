Program ::= LINE*

LINE ::= ASSIGNMENT | PARAMETER | STATEMENT | FUNCTION | COMMENT


ASSIGNMENT ::= BLOCK
PARAMETER ::= ( 'SET' SETTING '=' VALUE ) | ( 'CONSTANT' VARIABLE '=' VALUE )
STATEMENT ::= VARIABLE '=' FUNCTION '(' ( VARIABLE | ("," VARIABLE) )? ')'

FUNCTION ::= "FUNCTION"  NAME DECLARATION   | STATEMENT | RETURN
COMMENT ::= "#" (ALPHA|NUMBER)*

ALPHA ::= Letter*
NUMBER ::= Digit*
VARIABLE ::= ALPHA ( ALPHA | NUMBER )*
FUNCTION ::= DECLARATION  STATEMENT*


/*
CONSTANT ::== NUMBER
FUNCTION
NUMBER
ALPHA
VARIBALE ::= ALPHA NUMBER
COMMENT
STATEMENT ::= ( "ABS" | )
PredefinedEntityRef	   ::=   	"&" ("lt" | "gt" | "amp" | "quot" | "apos") ";"

*/
