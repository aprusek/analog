Program ::= LINE*

LINE ::= ASSIGNMENT | PARAMETER | STATEMENT | FUNCTION | COMMENT


ASSIGNMENT ::= BLOCK
PARAMETER ::= ( 'SET' SETTING '=' VALUE ) | ( 'CONSTANT' VARIABLE '=' VALUE )
STATEMENT ::= VARIABLE '=' FUNCTION '(' ( VARIABLE | ("," VARIABLE) )? ')'

FUNCTION ::= "FUNCTION"  NAME DECLARATION   | STATEMENT | RETURN
COMMENT ::= "#" (ALPHA|NUMBER)*

ALPHA ::= Letter*
NUMBER ::= Digit*
VARIABLE ::= ALPHA ( ALPHA | NUMBER )*
FUNCTION ::= DECLARATION  STATEMENT*


/*
CONSTANT ::== NUMBER
FUNCTION
NUMBER
ALPHA
VARIBALE ::= ALPHA NUMBER
COMMENT
STATEMENT ::= ( "ABS" | )
PredefinedEntityRef	   ::=   	"&" ("lt" | "gt" | "amp" | "quot" | "apos") ";"

*/


this works

program ::= line*

line ::= definition | assignment | function_def | comment
definition ::= 'set' value '=' char char*
assingment ::= node '=' function 
function ::= function_name '(' node ( ',' node )* ')'

function_def ::= 'function' function_name '(' node ( ',' node )* ')' 'NL'
                ( function_name '(' node ( ',' node )* ')' )+ 'NL'
                'return (' node ')'

comment ::= '#' char*

node ::= alpha ( alpha | digits)*
alpha ::= [A-Za-z]
digit ::= [0-9]
char ::= [#x20-#x7f]
